{
  "name": "execute-me",
  "version": "0.2.0",
  "description": "Automatically generate command line interfaces from JavaScript functions.",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch --notify",
    "lint": "run-s lint:prettier lint:tslint",
    "lint:prettier": "prettier-check '{src,test}/**/*.ts'",
    "lint:tslint": "tslint -t stylish -p .",
    "fix": "run-s fix:tslint fix:prettier",
    "fix:prettier": "prettier --write '{src,test}/**/*.ts'",
    "fix:tslint": "tslint --fix -t stylish -p .",
    "build": "rimraf lib && tsc --build tsconfig.build.json"
  },
  "author": "Means88",
  "license": "MIT",
  "jest": {
    "collectCoverage": true,
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.ts"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "./tsconfig.json"
      }
    }
  },
  "dependencies": {
    "@babel/parser": "^7.1.0",
    "@babel/types": "^7.1.0",
    "commander": "^2.18.0",
    "doctrine": "^2.1.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.1.0",
    "@types/chai": "^4.1.6",
    "@types/doctrine": "^0.0.3",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.11.0",
    "@types/sinon": "^5.0.2",
    "chai": "^4.2.0",
    "jest": "^23.6.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.3",
    "prettier-check": "^2.0.0",
    "rimraf": "^2.6.2",
    "sinon": "^6.3.4",
    "ts-jest": "^23.10.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.0.3"
  },
  "files": [
    "lib"
  ],
  "directories": {
    "example": "example",
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Means88/execute-me.git"
  },
  "keywords": [
    "cli"
  ],
  "bugs": {
    "url": "https://github.com/Means88/execute-me/issues"
  },
  "homepage": "https://github.com/Means88/execute-me#readme"
}

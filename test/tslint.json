{
    "extends": ["../tslint.json"],
    "rules": {
      "completed-docs": false,
      "no-non-null-assertion": false,
      "no-implicit-dependencies": [true, "dev"],
      "no-unused-expression": false,
      "no-unsafe-any": false,
      "jsdoc-format": false,
      "typedef": false,
      "object-literal-sort-keys": false
    }
  }
